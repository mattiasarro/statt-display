<script type="text/javascript">
  
  data_start = "<%= @timeframe.from %>";
  data_end   = "<%= @timeframe.to %>";
  
  from = <%= @timeframe.from.to_i %>;
  bar_duration = <%= graph.bar_duration.to_i %>;
  nr_bars = <%= graph.nr_bars %>;
  max_value = <%= graph.data.values.max || 0 %>;
  
  <% "can't do graph.data.each because we need to fill out empty bars as well" %>
  
  data = [];
  <% graph.data.each do |k,v| %>
    data.push({time: <%= k %>, value: <%= v %>});
  <% end %>
  
  
  loads = <%= raw @loads.page(@ui.loads_pg).to_json %>;
  
</script>

