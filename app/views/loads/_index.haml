.info-header
  %ul.nav.nav-tabs
    %li
      %a{href: "#"} Visitors
    %li.active
      %a{href: "#"} Loads
    %li
      %a{href: "#"} Nodes
  .pagination
    %ul
      %li= link_to "«", '#'
      - @ui.loads_pages.each do |page|
        %li{class: ('active' if page.current?)}
          = link_to page.nr, page.uri_hash
          
      %li.disabled= link_to "»", '#'

.loads
  .loads-col
  .loads-col
  .loads-col
    - if false
      .load
        %span.uid &nbsp;
        %span.time 18:30
        %span.top /path/to/blog/post